{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020", "dom"],
    "noEmit": true,
    "module": "es2020",
    "strict": false,
    "moduleResolution": "node16",
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"],
  "include": [
    "*.js",
    "src/**/*.js",
    "src/**/*.d.ts"
  ]
}
